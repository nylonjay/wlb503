<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:orientation="vertical"
    tools:context="com.bankscene.bes.welllinkbank.activity.finance.quoteprice.ForeignQuotePrice">
    <com.bankscene.bes.welllinkbank.view.translucent.TranslucentActionBar
        android:id="@+id/actionBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:type="1"
        />
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/base_function_block"
        android:padding="8dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp">
        
        <TextView
            android:id="@+id/tv_excel_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/greyText"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="10dp"
            android:text="@string/excel_update_time"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/background"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="3dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">
        <TextView
            style="@style/TextBigBlack"
            android:layout_width="0dp"
            android:gravity="center"
            android:textColor="@color/blue_title"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/currency"
            />
        <TextView
            style="@style/TextBigBlack"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/blue_title"
            android:layout_weight="1"
            android:maxLines="2"
            android:gravity="center"
            android:text="@string/tel_in_price"
            />
        <TextView
            style="@style/TextBigBlack"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/blue_title"
            android:layout_weight="1"
            android:maxLines="2"
            android:gravity="center"
            android:text="@string/tel_out_price"
            />
        <TextView
            style="@style/TextBigBlack"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/blue_title"
            android:layout_weight="1"
            android:maxLines="2"
            android:gravity="center"
            android:text="@string/oof_in_price"
            />

        <TextView
            style="@style/TextBigBlack"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/blue_title"
            android:layout_weight="1"
            android:maxLines="2"
            android:gravity="center"
            android:text="@string/oof_out_price"
            />

    </LinearLayout>

        <ListView
            android:id="@+id/list_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginBottom="8dp"
            android:divider="@null" />

    </LinearLayout>
</LinearLayout>
