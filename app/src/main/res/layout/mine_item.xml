<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        android:id="@+id/re_content"
        android:layout_width="match_parent"
        android:layout_height="@dimen/mine_item_height"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        >
        <ImageView
            android:id="@+id/img_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"/>
        <TextView
            android:id="@+id/tv_txt"
            style="@style/TextWrapBlack"
            android:text="个人信息"
            android:layout_toRightOf="@+id/img_icon"
            android:textSize="@dimen/mainitem_size"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/icon_txt_margin"
            />
            <com.bankscene.bes.welllinkbank.view.IconFontTextView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                app:fontFile="welllbk.ttf"
                app:value="@string/wlb_arrow_r"/>


    </RelativeLayout>
    <View
        android:id="@+id/view_divider"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/background"
        android:layout_marginLeft="10dp"
        android:layout_alignParentBottom="true" />
    <View
        android:id="@+id/view_empty"
        android:layout_width="match_parent"
        android:background="@color/background"
        android:layout_alignParentBottom="true"
        android:layout_height="15dp"/>
</LinearLayout>
